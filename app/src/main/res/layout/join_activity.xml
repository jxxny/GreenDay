<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFFFF">

    <Button
        android:id="@+id/nickname_check_button"
        android:layout_width="75dp"
        android:layout_height="52dp"
        android:layout_marginTop="67dp"
        android:layout_marginEnd="59dp"
        android:layout_marginRight="59dp"
        android:background="@drawable/full_view_layout"
        android:elevation="20dp"
        android:fontFamily="@font/nanum_eb"
        android:text="중복확인"
        android:textColor="#FFFFFF"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/insert_nickname"
        app:layout_constraintTop_toBottomOf="@+id/line" />

    <EditText
        android:id="@+id/insert_repwd"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="40dp"
        android:layout_marginBottom="312dp"
        android:background="@drawable/view_layout"
        android:fontFamily="@font/nanum"
        android:hint="비밀번호를 한 번 더 입력해주세요."
        android:inputType="textPassword"
        android:paddingLeft="20dp"
        android:paddingTop="18dp"
        android:paddingRight="88dp"
        android:paddingBottom="18dp"
        android:textColorHint="#A8A8A8"
        android:textSize="14sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.513"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/insert_pwd"
        app:layout_constraintVertical_bias="0.035" />

    <TextView
        android:id="@+id/Q_reinsert_pwd"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        android:fontFamily="@font/nanum"
        android:text="비밀번호 재확인"
        android:textColor="#000000"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.183"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/insert_pwd"
        app:layout_constraintVertical_bias="0.007" />

    <ImageView
        android:id="@+id/back_arrow"
        android:layout_width="22dp"
        android:layout_height="28dp"
        android:layout_marginTop="26dp"
        app:layout_constraintEnd_toStartOf="@+id/join_title"
        app:layout_constraintHorizontal_bias="0.125"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/arrow" />

    <TextView
        android:id="@+id/join_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="30dp"
        android:layout_marginBottom="676dp"
        android:fontFamily="@font/nanum_eb"
        android:gravity="center"
        android:text="Green Day"
        android:textColor="#7BC77D"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        app:lineHeight="28sp" />

    <TextView
        android:id="@+id/Q_insert_id"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="8dp"
        android:fontFamily="@font/nanum"
        android:text="아이디"
        android:textColor="#000000"
        app:layout_constraintBottom_toTopOf="@+id/insert_id"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.155"
        app:layout_constraintStart_toStartOf="parent" />

    <EditText
        android:id="@+id/insert_id"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="55dp"
        android:layout_marginLeft="55dp"
        android:layout_marginBottom="404dp"
        android:background="@drawable/view_layout"
        android:fontFamily="@font/nanum"
        android:hint="아이디를 입력해주세요."
        android:paddingLeft="20dp"
        android:paddingTop="18dp"
        android:paddingRight="60dp"
        android:paddingBottom="18dp"
        android:textColorHint="#A8A8A8"
        android:textSize="14sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/id_check_button"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/join_title"
        app:layout_constraintVertical_bias="0.451" />

    <TextView
        android:id="@+id/Q_insert_pwd"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="8dp"
        android:fontFamily="@font/nanum"
        android:text="비밀번호"
        android:textColor="#000000"
        app:layout_constraintBottom_toTopOf="@+id/insert_pwd"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.162"
        app:layout_constraintStart_toStartOf="parent" />

    <EditText
        android:id="@+id/insert_pwd"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="59dp"
        android:layout_marginLeft="59dp"
        android:layout_marginTop="40dp"
        android:layout_marginEnd="77dp"
        android:layout_marginRight="60dp"
        android:background="@drawable/view_layout"
        android:fontFamily="@font/nanum"
        android:hint="비밀번호를 입력해주세요."
        android:inputType="textPassword"
        android:paddingLeft="20dp"
        android:paddingTop="18dp"
        android:paddingRight="134dp"
        android:paddingBottom="18dp"
        android:textColorHint="#A8A8A8"
        android:textSize="14sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/id_check_button" />

    <Button
        android:id="@+id/id_check_button"
        android:layout_width="75dp"
        android:layout_height="52dp"
        android:layout_marginTop="24dp"
        android:layout_marginEnd="55dp"
        android:layout_marginRight="55dp"
        android:background="@drawable/full_view_layout"
        android:elevation="20dp"
        android:fontFamily="@font/nanum_eb"
        android:text="중복확인"
        android:textColor="#FFFFFF"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/insert_id"
        app:layout_constraintTop_toTopOf="@+id/Q_insert_id" />

    <ImageView
        android:id="@+id/line"
        android:layout_width="299dp"
        android:layout_height="8dp"
        android:layout_marginStart="29dp"
        android:layout_marginLeft="29dp"
        android:layout_marginTop="80dp"
        android:layout_marginEnd="30dp"
        android:layout_marginRight="30dp"
        android:layout_marginBottom="298dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.547"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/insert_repwd"
        app:srcCompat="@drawable/line" />

    <TextView
        android:id="@+id/Q_nickname"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="59dp"
        android:layout_marginLeft="59dp"
        android:layout_marginTop="40dp"
        android:layout_marginEnd="315dp"
        android:layout_marginRight="315dp"
        android:fontFamily="@font/nanum"
        android:text="닉네임"
        android:textColor="#000000"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/line" />

    <EditText
        android:id="@+id/insert_nickname"
        android:layout_width="208dp"
        android:layout_height="52dp"
        android:layout_marginStart="55dp"
        android:layout_marginLeft="55dp"
        android:layout_marginTop="12dp"
        android:background="@drawable/view_layout"
        android:ems="10"
        android:fontFamily="@font/nanum"
        android:hint="닉네임을 입력해주세요."
        android:inputType="textPersonName"
        android:paddingLeft="20dp"
        android:paddingTop="18dp"
        android:paddingRight="60dp"
        android:paddingBottom="18dp"
        android:textColorHint="#A8A8A8"
        android:textSize="14sp"
        app:layout_constraintEnd_toStartOf="@+id/nickname_check_button"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Q_nickname" />

    <Button
        android:id="@+id/join_button"
        android:layout_width="286dp"
        android:layout_height="50dp"
        android:layout_marginStart="62dp"
        android:layout_marginLeft="62dp"
        android:layout_marginTop="29dp"
        android:layout_marginEnd="63dp"
        android:layout_marginRight="63dp"
        android:background="@drawable/full_view_layout"
        android:elevation="20dp"
        android:fontFamily="@font/nanum_eb"
        android:text="가입하기"
        android:textColor="#FFFEFE"
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/insert_nickname" />

    <ImageView
        android:id="@+id/waste_re"
        android:layout_width="317dp"
        android:layout_height="62dp"
        android:layout_marginStart="47dp"
        android:layout_marginLeft="47dp"
        android:layout_marginTop="15dp"
        android:layout_marginEnd="47dp"
        android:layout_marginRight="47dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/insert_repwd"
        app:srcCompat="@drawable/waste" />

    <ImageView
        android:id="@+id/setImage"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="28dp"
        android:layout_marginRight="28dp"
        app:layout_constraintBottom_toBottomOf="@+id/insert_repwd"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/insert_repwd"
        app:srcCompat="@drawable/setimage" />


</androidx.constraintlayout.widget.ConstraintLayout>